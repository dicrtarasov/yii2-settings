{
	"name" : "dicr/yii2-settings",
	"description" : "Settings component for Yii2",
	"type" : "yii2-extension",
	"authors" : [{
			"name" : "Igor Tarasov",
			"email" : "develop@dicr.org",
			"homepage" : "http://dicr.org",
			"role" : "developer"
		}
	],
	"minimum-stability" : "stable",
	"require" : {
		"php" : ">=7.0",
		"yiisoft/yii2" : "~2.0",
		"dicr/yii2-helper" : "~2.0"
	},
	"autoload" : {
		"psr-4" : {
			"dicr\\settings\\" : "src"
		}
	},
	"keywords" : [
		"php yii2 settings"
	],
	"repositories" : [{
			"type" : "composer",
			"url" : "https://asset-packagist.org"
		}, {
			"type" : "git",
			"url" : "https://gitlab.com/dicr/yii2-helper.git",
			"name" : "yii2-helper"
		}
	],
	"homepage" : "https://gitlab.com/dicr/yii2-settings",
	"support" : {
		"source" : "https://gitlab.com/dicr/yii2-settings.git"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~6",
		"ext-sqlite3" : "*",
		"ext-yaml" : "*"
	}
}