{
    "name": "dicr/yii2-settings",
    "license": "MIT",
    "description": "Компонент для сохранения настроек Yii2",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "keywords": ["dicr", "php", "yii2", "settings"],
    "homepage": "https://gitlab.com/dicr/yii2-settings",
    "support": {
        "source": "https://gitlab.com/dicr/yii2-settings.git"
    },
    "autoload": {
        "psr-4": {
            "dicr\\settings\\": "src"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "~2.0.32",
        "dicr/yii2-helper": "~2.1.9"
    },
    "suggest": {
        "ext-yaml": "Needed form YML settings store"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~7.5.20",
        "ext-sqlite3": "*",
        "ext-yaml": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }, {
            "type": "git",
            "url": "https://gitlab.com/dicr/yii2-helper.git",
            "name": "yii2-helper"
        }
    ]
}